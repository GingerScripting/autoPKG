<?xml version="1.0" encoding="UTF-8"?>
  <policy>
    <general>
      <name>%POLICY_NAME% -2</name>
      <enabled>false</enabled>
      <trigger_other>install-%NAME%</trigger_other>
      <frequency>Ongoing</frequency>
    <category>
        <name>Master App Installs</name>
    </category>
    </general>
    <scope>
      <all_computers>false</all_computers>
      <computer_groups>
        <computer_group>
          <id>1</id>
          <name>All Managed Clients</name>
        </computer_group>
      </computer_groups>
      <exclusions>
        <computers/>
        <computer_groups>
          <computer_group>
            <id>17</id>
            <name>ZR - All Zoom Rooms Computers</name>
          </computer_group>
        </computer_groups>
      </exclusions>
    </scope>
    <package_configuration>
      <packages>
        <size>1</size>
        <package>
          <name>%PKG_NAME%</name>
          <action>Install</action>
        </package>
      </packages>
    </package_configuration>
  </policy>
